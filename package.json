{
  "name": "eslint-plugin-backpack",
  "version": "1.1.0",
  "description": "ESlint configuration for awesome Backpack development.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Backpack Design System <backpack@skyscanner.net>",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:Skyscanner/eslint-plugin-backpack.git"
  },
  "engines": {
    "node": "^12.13.0",
    "npm": "^6.12.0"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npx ensure-node-env",
    "test": "mocha src/*.test.js src/**/*.test.js",
    "lint:js:fix": "eslint . --ext js,jsx --fix",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,jsx}\"",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "bpk-tokens": "^32.0.2",
    "lodash": "^4.17.16",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^5.13.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint_d": "^8.0.0",
    "mocha": "^7.0.1",
    "prettier": "^1.19.1",
    "snyk": "^1.327.0"
  },
  "snyk": true
}
