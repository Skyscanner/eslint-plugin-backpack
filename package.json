{
  "name": "eslint-plugin-backpack",
  "version": "0.2.1",
  "description": "ESlint configuration for awesome Backpack development.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Backpack Design System <backpack@skyscanner.net>",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:Skyscanner/eslint-plugin-backpack.git"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "mocha src/*.test.js src/**/*.test.js",
    "lint:js:fix": "eslint . --ext js,jsx --fix",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,jsx}\""
  },
  "license": "Apache-2.0",
  "dependencies": {
    "bpk-tokens": "^27.0.4",
    "lodash": "^4.17.10",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "eslint": "^4.19.1",
    "eslint-config-skyscanner-with-prettier": "^0.2.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-react": "^7.10.0",
    "eslint_d": "^6.0.0",
    "mocha": "^5.2.0"
  }
}
