{
  "name": "eslint-plugin-backpack",
  "version": "0.2.2",
  "description": "ESlint configuration for awesome Backpack development.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Backpack Design System <backpack@skyscanner.net>",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:Skyscanner/eslint-plugin-backpack.git"
  },
  "engines": {
    "node": "^8.9.0",
    "npm": "^6.4.1"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npx ensure-node-env",
    "test": "mocha src/*.test.js src/**/*.test.js",
    "lint:js:fix": "eslint . --ext js,jsx --fix",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,jsx}\""
  },
  "license": "Apache-2.0",
  "dependencies": {
    "bpk-tokens": "^27.0.4",
    "lodash": "^4.17.10",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-skyscanner-with-prettier": "^0.4.0",
    "eslint_d": "^7.1.0",
    "mocha": "^5.2.0"
  }
}
